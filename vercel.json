{
  "routes": [
    {
      "src": "/_next/static/(.*)",
      "dest": "/_next/static/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/([^/]+\\.(?:jpg|jpeg|png|gif|svg|webp|ico|webmanifest|xml|txt))$",
      "dest": "/$1"
    },
    {
      "src": "^/favicon.ico$",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/robots.txt$",
      "dest": "/robots.txt"
    },
    {
      "src": "^/sitemap.xml$",
      "dest": "/sitemap.xml"
    },
    {
      "src": "^/manifest.json$",
      "dest": "/manifest.json"
    },
    {
      "src": "/article-1",
      "dest": "/index.html"
    },
    {
      "src": "/article-2",
      "dest": "/index.html"
    },
    {
      "src": "/article-3",
      "dest": "/index.html"
    },
    {
      "src": "/article-4",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
